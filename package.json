{
  "name": "n8n-nodes-browserflow",
  "version": "0.3.5",
  "description": "Browserflow for LinkedIn is a toolkit for automating actions in LinkedIn. It allows you to automate actions like sending connection requests, sending messages, and more.",
  "keywords": [
    "n8n-community-node-package",
    "browserflow",
    "linkedin automation",
    "linkedin",
    "n8n"
  ],
  "license": "MIT",
  "homepage": "https://browserflow.io/n8n",
  "author": {
    "name": "Browserflow",
    "email": "hello@browserflow.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Browserflow-io/browserflow-n8n.git"
  },
  "engines": {
    "node": ">=18.10",
    "pnpm": ">=9.1"
  },
  "packageManager": "pnpm@9.1.4",
  "main": "index.js",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "pnpm build && pnpm lint -c .eslintrc.prepublish.js nodes credentials package.json"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/BrowserflowApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Browserflow/Browserflow.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.15.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n": "^1.110.1",
    "prettier": "^3.3.2",
    "typescript": "^5.5.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}